{
  "name": "hubla-challenge-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npx prisma generate && npx @nestjs/cli build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:debug:docker": "nest start --debug='0.0.0.0:9229'",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --maxWorkers=1 --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:staged": "yarn test --bail --findRelatedTests --passWithNoTests",
    "test:ci": "jest --coverage --detectOpenHandles",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles",
    "jest:clear": "jest --clearCache",
    "prisma": "prisma",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:db:migrate": "prisma migrate dev --name",
    "prisma:db:push": "prisma db push",
    "prisma:db:seed": "prisma db seed",
    "up:packs": "yarn ncu -u",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.1.4",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.1.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.1.2",
    "@prisma/client": "^4.3.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "date-fns": "^2.29.3",
    "express-rate-limit": "^6.6.0",
    "faker": "5.5.3",
    "faker-br": "^0.4.1",
    "helmet": "^6.0.0",
    "ioredis": "^5.2.3",
    "lodash": "^4.17.21",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "prisma": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.13",
    "@types/faker": "5.5.9",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.185",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.1",
    "git-commit-msg-linter": "^4.1.3",
    "husky": "^8.0.1",
    "jest": "^27.2.5",
    "jest-extended": "^3.1.0",
    "lint-staged": "^13.0.3",
    "npm-check-updates": "^16.2.1",
    "prettier": "^2.7.1",
    "prisma-erd-generator": "^1.2.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "collectCoverage": true,
    "maxConcurrency": 1,
    "collectCoverageFrom": [
      "*/.ts",
      "!*/.d.ts",
      "!<rootDir>/*.ts",
      "!<rootDir>/**/index.ts",
      "!<rootDir>/*/.module.ts",
      "!<rootDir>/config/*/",
      "!<rootDir>/common/*/"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/$1"
    }
  }
}
